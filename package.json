{
	"name": "n8n-nodes-agent-brains",
	"version": "0.1.0",
  "description": "AgentBrains nodes for n8n to integrate workflows, production webhooks, and Synthetic User validation with the AgentBrains platform.",
	"license": "MIT",
	"homepage": "",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "agent-brains",
    "agentbrains",
    "integration",
    "trigger",
    "webhook",
    "synthetic-users",
    "ai-employees"
  ],
	"author": {
		"name": "Vlad Balabash",
		"email": "vbalabash@agent-brains.com"
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.od.atncorp.com/datawizard/interfaces-and-gateways/n8n-custom-trigger-node.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/AgentBrainsIntegrationApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/AgentBrainsIntegrationTrigger/AgentBrainsIntegrationTrigger.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"express": "^5.1.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
